packageVersion("pegas")
packageVersion("haplotypes")
library(pegas)
library(haplotypes)
path = "/Users/niuguigui/Documents/its2/ITS2文章附件20190527/6.haplotype/cladeA-I_seq"
setwd(path)
for(i in 1:9){
#拼接文件名称
fileName="cladeC_3.aln.fas"
#读取alignment后的序列
seq <- read.fas(file=fileName)
#sic模式生成haplotype
hsic<-haplotype(seq,indels="sic")
#hsic@d距离矩阵
dist<-hsic@d
#将矩阵存为csv文件
write.csv(dist,paste(fileName,".matrix.csv",sep=""))
#矩阵转为mst
M <- mst(dist)
#将mst存为csv文件
#csv文件中第一列为编号，无实际意义
#第二列及第三列为序列的序号，序号对应alignment文件序列的顺序
#第二列、第三列为配对的序列，第四列为碱基差异数
write.csv(M,paste(fileName,".mst.csv",sep=""))
}
